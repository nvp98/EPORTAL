@model EPORTAL.ModelsPartner.VehicleManagementValidation

@using (Html.BeginForm("Update_File", "VehicleManagement", FormMethod.Post, new { enctype = "multipart/form-data", onsubmit = "return myFunction()" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.IDTBNT)
    @Html.HiddenFor(model => model.IDLoai)
    <link href="~/Content/assets/Chonse/chosen.css" rel="stylesheet" />
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 id="ModalTitle"></h5>
                <a href="#" class="close" data-dismiss="modal">&times;</a>
            </div>
            <div class="modal-body">
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <fieldset id="SubmitForm">
                    <div class="form-row">


                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="small mb-1" for="inputLastName">Nhập file Import </label>
                                <input type="file" name="NameFile" class="form-control" />
                            </div>
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-12">
                            <button class="btn btn-primary pull-right" id="btnSubmit"><i class="fa fa-check-circle"></i> Xác nhận</button>
                        </div>
                    </div>
                </fieldset>

            </div>
        </div>
    </div>
}
<script src="~/Content/assets/Chonse/chosen.jquery.js"></script>



