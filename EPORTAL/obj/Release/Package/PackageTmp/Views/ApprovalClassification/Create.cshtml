
@model EPORTAL.Models.ApprovalClassification
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
@using (Html.BeginForm("Create", "ApprovalClassification", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

            <div class="modal-dialog" id="form">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 id="ModalTitle"></h5>
                        <a href="#" class="close" data-dismiss="modal">&times;</a>
                    </div>
                    <div class="modal-body">


                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                        <fieldset id="SubmitForm">
                            <div class="row">


                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label class="small mb-1">Tên Loại Ký Duyệt</label>
                                        @Html.EditorFor(model => model.tenloaikyduyet, new { htmlAttributes = new { @class = "form-control", placeholder = "Tên Loại Ký Duyệt", @id = "tenloaikyduyet" } })
                                        @Html.ValidationMessageFor(model => model.tenloaikyduyet, "", new { @class = "text-danger" })

                                    </div>
                                </div>
                            </div>



                            <div class="btn-margin-top text-center">
                                <input type="submit" value="Xác nhận" class="btn btn-success button_submit" />

                            </div>
                        </fieldset>

                    </div>
                </div>
            </div>
}











